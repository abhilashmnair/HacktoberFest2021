#include<iostream> 
using namespace std;
int main()
{
	int A[3][3] = {{1,2,3}, {4,5,6}, {7,8,9}};
	int i,j;

	for(i=0; i<3; i++)
	{
		for(j=i; j<3; j++)
			{
				//swap
				int temp = A[i][j];
				A[i][j] = A[j][i];
				A[j][i] = temp;
			}	
	}

	//print 
  for(i=0; i<3; i++)
	{
		for(j=0; j<3; j++)
			{	
				cout << A[i][j] << " " ;
			}
		cout << endl;
	}
	return 0;
}
